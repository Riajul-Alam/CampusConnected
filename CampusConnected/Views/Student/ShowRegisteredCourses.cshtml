@model IEnumerable<dynamic>

<!DOCTYPE html>
<html>
<head>
    <title>Show Registered Courses</title>
</head>
<body>
    <h1>Registered Courses</h1>

    <table>
        <thead>
            <tr>
                <th>Course Name</th>
                <th>Course Code</th>
                <th>Semester</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var course in Model)
            {
                <tr>
                    <td>@course.CourseName</td>
                    <td>@course.CourseCode</td>
                    <td>@course.Semester</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>

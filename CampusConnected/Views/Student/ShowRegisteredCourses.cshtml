<!-- Assuming Course is your entity/model class for courses -->
@model List<CampusConnected.Models.Course>
<h2>Registered Courses</h2>

@if (Model.Count == 0)
{
    <p>No courses registered.</p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Course ID</th>
                <th>Course Name</th>
                <th>Course Code</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var course in Model)
            {
                <tr>
                    <td>@course.CId</td>
                    <td>@course.CourseName</td>
                    <td>@course.CourseCode</td>
                </tr>
            }
        </tbody>
    </table>
}
